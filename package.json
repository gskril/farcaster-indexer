    "node-cron": "3.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^18.7.21",
    "prettier": "^2.8.1",
    "typescript": "^4.8.3"
